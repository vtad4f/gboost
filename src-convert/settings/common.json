{
   "post common" : {
      "prefix" : [
         "from my_src import *"
      ],
      "replace" : {
         "num2str" : "str",
         "length"  : "len"
      }
   },
   "post rename" : {
      "prefix" : [
         "from pylab import *"
      ],
      "multi-line-regex" : {
         ";$"                : "",
         "%"                 : "#",
         "\\s*\\.\\.\\.\\s*" : " "
      },
      "single-line-regex" : {
         "@\\(([^)]+)\\)" : "lambda {0}:"
      }
   },
   "post smop" : {
      "replace" : {
         "    "          : "   ",
         "libsmop"       : "smop.libsmop",
         "load"          : "my_load",
         "disp(concat("  : "disp(''.join(",
         "error(concat(" : "error(''.join(",
         "concat(['"     : "''.join(['",
         "concat("       : "my_concat(",
         "disp"          : "my_disp",
         "sort("         : "my_sort(",
         "norm("         : "my_norm(",
         "pause"         : "my_pause()"
      },
      "single-line-regex" : {
         ",nargout=[0-9]+" : ""
      }
   }
}