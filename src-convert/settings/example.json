{
   "post common" : {
      "prefix" : [
         "from gboost2 import gboost2"
      ],
      "replace" : {
         "pause" : "my_pause()"
      }
   },
   "post rename" : {
      "single-line-regex" : {
         "load ([A-Za-z0-9._-]+)" : "my_load('{0}', globals())"
      }
   },
   "pre smop" : {
      "replace" : {
         "example-graphs.mat" : "example_graphs"
      }
   },
   "post smop" : {
      "replace" : {
         "   "              : "",
         "example_graphs')" : "example-graphs.mat', globals())"
      }
   }
}